    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10v"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.97" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/inputField"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="@string/input_field"
        android:textAlignment="textEnd"
        android:textSize="@dimen/result_text_size_land"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintBottom_toTopOf="@id/buttonC"
        app:layout_constraintEnd_toEndOf="@id/buttonPlus"
        app:layout_constraintStart_toStartOf="@id/buttonC" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonC"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:insetLeft="@dimen/button_inset"
        android:insetRight="@dimen/button_inset"
        android:insetBottom="@dimen/button_inset"
        android:text="@string/C"
        android:textSize="@dimen/button_text_size_land"
        app:layout_constraintBottom_toTopOf="@id/button2"
        app:layout_constraintEnd_toStartOf="@id/buttonEraseToTheLeft"
        app:layout_constraintStart_toStartOf="@id/button1" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonEraseToTheLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:insetLeft="@dimen/button_inset"
        android:insetRight="@dimen/button_inset"
        android:insetBottom="@dimen/button_inset"
        android:text="@string/eraseToTheLeft"
        android:textSize="@dimen/button_text_size_land"
        app:layout_constraintBottom_toTopOf="@id/button3"
