        android:layout_height="wrap_content"
        android:insetLeft="@dimen/button_inset"
        android:insetRight="@dimen/button_inset"
        android:insetBottom="@dimen/button_inset"
        android:text="@string/division"
        android:textSize="@dimen/button_text_size_land"
        app:layout_constraintBottom_toTopOf="@id/buttonMultiply"
        app:layout_constraintEnd_toEndOf="@id/buttonMultiply"
        app:layout_constraintStart_toEndOf="@id/buttonEraseToTheLeft" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:insetLeft="@dimen/button_inset"
        android:insetTop="@dimen/button_inset"
        android:insetRight="@dimen/button_inset"
        android:insetBottom="@dimen/button_inset"
        android:text="@string/_1"
        android:textSize="@dimen/button_text_size_land"
        app:layout_constraintBottom_toTopOf="@id/button4"
        app:layout_constraintEnd_toStartOf="@id/button2"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/guideline5v"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
